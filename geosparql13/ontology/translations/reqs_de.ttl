reqs10gx:gml-literal rdf:type 
                              spec:Requirement ;
                     skos:definition "Alle geo:gmlLiterale müssen aus einem gültigen Element des GML-Schemas bestehen, das einen Subtyp von GM_Object, wie in [OGC 07-036] definiert, implementiert."@de ;
                     skos:prefLabel "GeoSPARQL 1.0 Anforderung: GML Literal"@de .

reqs10gx:gml-literal-empty rdf:type 
                                    spec:Requirement ;
                           skos:requires reqs10gx:gml-literal ;
                           skos:definition "Ein leeres geo:gmlLiteral ist als eine leere Geometrie zu interpretieren"@de ;
                           skos:prefLabel "GeoSPARQL 1.0 Anforderung: GML Literal Empty"@de .

reqs10gx:gml-profile rdf:type 
                              spec:Requirement ;
                     skos:definition "Implementierungen müssen die von ihnen unterstützten GML-Profile dokumentieren"@de ;
                     skos:prefLabel "GeoSPARQL 1.0 Anforderung: GML Profile"@de .

reqs10gx:query-functions rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementierungen müssen geof:distance, geof:buffer, geof:convexHull, geof:intersection, geof:union, geof:difference, geof:symDifference, geof:envelope und geof:boundary als SPARQL-Erweiterungsfunktionen unterstützen. Die Funktionen müssen in Übereinstimmung mit den Definitionen der entsprechenden Funktionen (distance, buffer, convexHull, intersection, difference, symDifference, envelope bzw. boundary) in Simple Features [ISO 19125-1] stehen."@de ;
                         skos:prefLabel "GeoSPARQL 1.0 Anforderung: Query Funktionen"@de .

reqs10gx:srid-function rdf:type 
                                spec:Requirement ;
                       skos:definition "Implementierungen müssen geof:getSRID als eine SPARQL-Erweiterungsfunktion unterstützen"@de ;
                       skos:prefLabel "GeoSPARQL 1.0 Anforderung: SRID Funktion"@de .

reqs10gx:wkt-axis-order rdf:type 
                                 spec:Requirement ;
                        skos:definition "Koordinatentupel in geo:wktLiterals sind unter Berücksichtigung der im verwendeten räumlichen Bezugssystem definierten Achsenfolge zu interpretieren"@de ;
                        skos:prefLabel "GeoSPARQL 1.0 Anforderung: WKT Axis Order"@de .

reqs10gx:wkt-literal rdf:type 
                              spec:Requirement ;
                     skos:definition "Alle RDFS-Literale vom Typ geo:wktLiteral bestehen aus einer optionalen URI, die das Koordinatenreferenzsystem identifiziert und einer Simple Features Well Known Text (WKT) Definition, die einen geometrischen Wert beschreibt. Gültige geo:wktLiterale werden durch Verkettung einer gültigen, absoluten URI gemäß [RFC 2396], einem oder mehreren Leerzeichen (U+0020) als Trennzeichen und einer WKT-Zeichenkette gemäß der Definition in Simple Features [ISO 19125-1] gebildet."@de ;
                     skos:prefLabel "GeoSPARQL 1.0 Anforderung: WKT Literal"@de .

reqs10gx:wkt-literal-default-srs rdf:type 
                                          spec:Requirement ;
                                 skos:definition "Die URI <http://www.opengis.net/def/crs/OGC/1.3/CRS84> wird als räumliches Bezugssystem für geo:wktLiterale angenommen, die keine expliziten URI für ein räumliches Bezugssystem definieren."@de ;
                                 skos:prefLabel "GeoSPARQL 1.0 Anforderung: WKT Literal Default SRS"@de .

reqs10gx:wkt-literal-empty rdf:type 
                                    spec:Requirement ;
                           skos:definition "Ein leeres RDFS-Literal des Typs geo:wktLiteral ist als leere Geometrie zu interpretieren"@de ;
                           skos:prefLabel "GeoSPARQL 1.0 Anforderung: WKT Literal Empty"@de .

reqs10gtx:eh-query-functions rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementierungen müssen geof:ehEquals, geof:ehDisjoint, geof:ehMeet, geof:ehOverlap, geof:ehCovers, geof:ehCoveredBy, geof:ehInside, geof:ehContains als SPARQL-Erweiterungsfunktionen unterstützen, die mit den entsprechenden DE9IM-Schnittmengenmustern übereinstimmen, wie in Simple Features [ISO 19125-1] definiert."@de ;
                             skos:prefLabel "GeoSPARQL 1.0 Anforderung: EH Query Funktionen"@de .

reqs10gtx:rcc8-query-functions rdf:type 
                                        spec:Requirement ;
                               skos:definition "Implementierungen müssen die Funktionen geof:rcc8eq, geof:rcc8dc, geof:rcc8ec, geof:rcc8po, geof:rcc8tppi, geof:rcc8tpp, geof:rcc8ntpp, geof:rcc8ntppi als SPARQL-Erweiterungsfunktionen unterstützen, die in Übereinstimmung mit ihren entsprechenden DE-9IM-Schnittmengenmustern stehen, wie in Simple Features [ISO 19125-1] definiert."@de ;
                               skos:prefLabel "GeoSPARQL 1.0 Anforderung: RCC8 Query Funktionen"@de .

reqs10gtx:relate-query-function rdf:type 
                                         spec:Requirement ;
                                skos:definition "Implementierungen müssen geof:relate als SPARQL-Erweiterungsfunktion unterstützen. Die Definition der geof:relate Funktion stimmt mit der Definition des relate Operators in der Simple Features [ISO 19125-1] Spezifikation überein."@de ;
                                skos:prefLabel "GeoSPARQL 1.0 Anforderung: Relate Query Funktion"@de .

reqs10gtx:sf-query-functions rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementierungen müssen geof:sfEquals, geof:sfDisjoint, geof:sfIntersects, geof:sfTouches, geof:sfCrosses, geof:sfWithin, geof:sfContains, geof:sfOverlaps als SPARQL-Erweiterungsfunktionen unterstützen, die mit den entsprechenden DE9IM-Schnittmengenmustern übereinstimmen, wie in Simple Features [ISO 19125-1] definiert."@de ;
                             skos:prefLabel "GeoSPARQL 1.0 Anforderung: SF Query Funktionen"@de .

reqs10qre:eh-query-rewrite rdf:type 
                                    spec:Requirement ;
                           skos:definition "Für den Vergleich von Basic Graph Pattern, wird die im RDFS Entailment Regime [W3C SPARQL Entailment] definierte Semantik für die RIF Regeln [W3C RIF Core] geor:ehEquals, geor:ehDisjoint, geor:ehMeet, geor:ehOverlap, geor:ehCovers, geor:ehCoveredBy, geor:ehInside, geor:ehContains verwendet"@de ;
                           skos:prefLabel "GeoSPARQL 1.0 Anforderung: EH Query Rewrite"@de .

reqs10qre:rcc8-query-rewrite rdf:type 
                                      spec:Requirement ;
                             skos:definition "Für den Vergleich von Basic Graph Pattern, wird die im RDFS Entailment Regime [W3C SPARQL Entailment] definierte Semantik für die RIF Regeln [W3C RIF Core] geor:rcc8eq, geor:rcc8dc, geor:rcc8ec, geor:rcc8po, geor:rcc8tppi, geor:rcc8tpp, geor:rcc8ntpp, geor:rcc8ntppi verwendet"@de ;
                             skos:prefLabel "GeoSPARQL 1.0 Anforderung: RCC8 Query Rewrite"@de .

reqs10qre:sf-query-rewrite rdf:type 
                                    spec:Requirement ;
                           skos:definition "Für den Vergleich von Basic Graph Pattern, wird die im RDFS Entailment Regime [W3C SPARQL Entailment] definierte Semantik für die RIF Regeln [W3C RIF Core] geor:sfEquals, geor:sfDisjoint, geor:sfIntersects, geor:sfTouches, geor:sfCrosses, geor:sfWithin, geor:sfContains, geor:sfOverlaps verwendet."@de ;
                           skos:prefLabel "GeoSPARQL 1.0 Anforderung: SF Query Rewrite"@de .

reqs10ree:bgp-rdfs-ent rdf:type 
                                spec:Requirement ;
                       skos:definition "Für den Vergleich von Basic Graph Pattern, wird die im RDFS Entailment Regime [W3C SPARQL Entailment] definierte Semantik verwendet."@de ;
                       skos:prefLabel "GeoSPARQL 1.0 Anforderung: BGP RDFS Entailment"@de .

reqs10ree:gml-geometry-types rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementierungen müssen Graph Pattern unterstützen, die Begriffe aus einer RDFS/OWL-Klassenhierarchie von Geometrietypen enthalten, die mit dem GML-Schema übereinstimmen, das GM_Object unter Verwendung der angegebenen GML-Version implementiert [OGC 07-036]."@de ;
                             skos:prefLabel "GeoSPARQL 1.0 Anforderung: GML Geometry Types"@de .

reqs10ree:wkt-geometry-types rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementierungen müssen Graph Pattern unterstützen, die Begriffe aus einer RDFS/OWL-Klassenhierarchie von Geometrietypen enthalten, die mit der in der spezifizierten Version von Simple Features [ISO 19125-1] übereinstimmen"@de ;
                             skos:prefLabel "GeoSPARQL 1.0 Anforderung: WKT Geometry Types"@de .

reqs10tve:eh-spatial-relations rdf:type 
                                       spec:Requirement ;
                               skos:definition "Implementierungen müssen die Verwendung der Eigenschaften geo:ehEquals, geo:ehDisjoint, geo:ehMeet, geo:ehOverlap, geo:ehCovers, geo:ehCoveredBy, geo:ehInside, geo:ehContains in SPARQL Graph Pattern ermöglichen"@de ;
                               skos:prefLabel "GeoSPARQL 1.0 Anforderung: EH Spatial Relations"@de .

reqs10tve:rcc8-spatial-relations rdf:type 
                                          spec:Requirement ;
                                 skos:definition "Implementierungen müssen die Verwendung der Eigenschaften geo:rcc8eq, geo:rcc8dc, geo:rcc8ec, geo:rcc8po, geo:rcc8tppi, geo:rcc8tpp, geo:rcc8ntpp, geo:rcc8ntppi in SPARQL Graph Pattern ermöglichen"@de ;
                                 skos:prefLabel "GeoSPARQL 1.0 Anforderung: RCC8 Spatial Relations"@de .

reqs10tve:sf-spatial-relations rdf:type 
                                        spec:Requirement ;
                               skos:definition "Implementierungen müssen die Verwendung der Eigenschaften geo:sfEquals, geo:sfDisjoint, geo:sfIntersects, geo:sfTouches, geo:sfCrosses, geo:sfWithin, geo:sfContains, geo:sfOverlaps in SPARQL Graph Pattern ermöglichen"@de ;
                               skos:prefLabel "GeoSPARQL 1.0 Anforderung: SF Spatial Relations"@de .

