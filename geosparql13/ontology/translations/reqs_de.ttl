conf11qre: rdf:type 
                    spec:ConformanceClass ;
           dcterms:requires confs11:geometry-topology-extension ;
           skos:prefLabel "GeoSPARQL 1.1 Konformitätsklasse: Query Rewrite Extension"@de .

reqs11core:spatial-object-collection-class rdf:type 
                                                    spec:Requirement ;
                                           skos:definition "Implementierungen müssen die Verwendung der RDFS-Klasse geo:SpatialObjectCollection in SPARQL Graph Pattern ermöglichen"@de ;
                                           skos:prefLabel "GeoSPARQL 1.1 Anforderung: Spatial Object Collection Class"@de .

reqs11core:feature-collection-class rdf:type 
                                             spec:Requirement ;
                                    skos:definition "Implementierungen müssen die Verwendung der RDFS-Klasse geo:FeatureCollection in SPARQL Graph Pattern ermöglichen"@de ;
                                    skos:prefLabel "GeoSPARQL 1.1 Anforderung: Feature Collection Class"@de .

reqs11core:geometry-collection-class rdf:type 
                                              spec:Requirement ;
                                     skos:definition "Implementierungen müssen die Verwendung der RDFS-Klasse geo:GeometryCollection in SPARQL Graph Pattern ermöglichen"@de ;
                                     skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry Collection Class"@de .

reqs11core:sparql-protocol rdf:type 
                                    spec:Requirement ;
                           skos:definition "Implementierungen müssen die SPARQL Query Language for RDF [SPARQL], das SPARQL Protokoll [SPARQLPROT] und die SPARQL Query Results XML [SPARQLRESX] und JSON [SPARQLRESJ] Formate unterstützen."@de ;
                           skos:prefLabel "GeoSPARQL 1.1 Anforderung: SPARQL Protocol"@de ;
                           prov:wasDerivedFrom reqs10core:sparql-protocol .

reqs11gx:dggs-literal rdf:type 
                               spec:Requirement ;
                      skos:definition "Alle RDFS-Literale des Typs geo:dggsLiteral müssen aus einer DGGS-Geometrieserialisierung bestehen, die entsprechend einem spezifischen DGGS-Literal-Typ formuliert ist, der durch einen Datentyp gekennzeichnet ist, der diesen generischen Datentyp spezialisiert"@de ;
                      skos:prefLabel "GeoSPARQL 1.1 Anforderung: DGGS Literal"@de .

reqs11gx:dggs-literal-empty rdf:type 
                                     spec:Requirement ;
                            skos:definition "Ein leeres geo:dggsLiteral ist als eine leere Geometrie zu interpretieren"@de ;
                            skos:prefLabel "GeoSPARQL 1.1 Anforderung: DGGS Literal Empty"@de .

reqs11gx:feature-properties rdf:type 
                                     spec:Requirement ;
                            skos:definition "Implementierungen müssen die Verwendung der Eigenschaften geo:hasGeometry, geo:hasDefaultGeometry, geo:hasLength, geo:hasArea, geo:hasVolume geo:hasCentroid, geo:hasBoundingBox und geo:hasSpatialResolution in SPARQL Graph Pattern ermöglichen"@de ;
                            skos:prefLabel "GeoSPARQL 1.1 Anforderung: Feature Properties"@de ;
                            prov:wasDerivedFrom reqs10gx:feature-properties .

reqs11gx:geojson-literal rdf:type 
                                  spec:Requirement ;
                         skos:definition "Alle geo:geoJSONLiteral-Instanzen müssen aus Geometrieobjekten bestehen, die in der GeoJSON-Spezifikation [GEOJSON] definiert sind"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal"@de .

reqs11gx:geojson-literal-empty rdf:type 
                                        spec:Requirement ;
                               skos:definition "Ein leeres geo:kmlLiteral ist als eine leere Geometrie zu interpretieren"@de ;
                               skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal Empty"@de .

reqs11gx:geojson-literal-srs rdf:type 
                                      spec:Requirement ;
                             skos:definition "RDFS-Literale des Typs geo:geoJSONLiteral enthalten keine SRS-Definition. Alle Literale dieses Typs sind gemäß der GeoJSON-Spezifikation nur in dem geodätischen Längen-Breiten-Raumbezugssystem WGS84 (http://www.opengis.net/def/crs/OGC/1.3/CRS84) kodiert, und es wird davon ausgegangen, dass dieses verwendet wird."@de ;
                             skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal Default SRS"@de .

reqs11gx:geometry-as-dggs-function rdf:type 
                                            spec:Requirement ;
                                   skos:definition "Implementierungen müssen geof:asDGGS als eine SPARQL-Erweiterungsfunktion unterstützen"@de ;
                                   skos:prefLabel "GeoSPARQL 1.1 Anforderung: asDGGS Funktion"@de .

reqs11gx:geometry-as-dggs-literal rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementierungen müssen die Verwendung der RDF-Eigenschaft geo:asDGGS in SPARQL Graph Pattern ermöglichen"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asDGGS Literal"@de .

reqs11gx:geometry-as-geojson-function rdf:type 
                                               spec:Requirement ;
                                      skos:definition "Implementierungen müssen geof:asGeoJSON als eine SPARQL-Erweiterungsfunktion unterstützen"@de ;
                                      skos:prefLabel "GeoSPARQL 1.1 Anforderung: asGeoJSON Funktion"@de .

reqs11gx:geometry-as-geojson-literal rdf:type 
                                              spec:Requirement ;
                                     skos:definition "Implementierungen müssen die Verwendung der RDF-Eigenschaft geo:asGeoJSON in SPARQL Graph Pattern ermöglichen"@de ;
                                     skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asGeoJSON Literal"@de .

reqs11gx:geometry-as-gml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementierungen müssen geof:asGML als SPARQL-Erweiterungsfunktion unterstützen"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asGML Funktion"@de .

reqs11gx:geometry-as-kml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementierungen müssen die Verwendung der RDF-Eigenschaft geo:asKML in SPARQL Graph Pattern ermöglichen"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asKML Funktion"@de .

reqs11gx:geometry-as-kml-literal rdf:type 
                                          spec:Requirement ;
                                 skos:definition "Implementierungen müssen die Verwendung der RDF-Eigenschaft geo:asKML in SPARQL Graph Pattern ermöglichen"@de ;
                                 skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asKML Literal"@de .

reqs11gx:geometry-as-wkt-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementierungen müssen geof:asWKT als SPARQL-Erweiterungsfunktion unterstützen"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asWKT Funktion"@de .

reqs11gx:geometry-properties rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementierungen müssen die Verwendung der Eigenschaften geo:dimension, geo:coordinateDimension, geo:spatialDimension, geo:isEmpty, geo:isSimple, geo:hasSerialization in SPARQL Graph Pattern ermöglichen"@de ;
                             skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry Properties"@de ;
                             prov:wasDerivedFrom reqs10gx:geometry-properties .

reqs11gx:kml-literal rdf:type 
                              spec:Requirement ;
                     skos:definition "Alle geo:kmlLiteral-Instanzen müssen aus den in der KML-Spezifikation [OGCKML] definierten Geometrieobjekten bestehen."@de ;
                     skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal"@de .

reqs11gx:kml-literal-empty rdf:type 
                                    spec:Requirement ;
                           skos:definition "Ein leeres geo:geoJSONLiteral ist als eine leere Geometrie zu interpretieren"@de ;
                           skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal Empty"@de .

reqs11gx:kml-literal-srs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Die IRI http://www.opengis.net/def/crs/OGC/1.3/CRS84 wird als SRS für geo:kmlLiterals angenommen, die keine explizite SRS-IRI angeben."@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal Default SRS"@de .

reqs11gx:query-functions rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementierungen müssen die Funktionen geof:boundary geof:buffer, geof:convexHull, geof:coordinateDimension, geof:difference, geof:dimension, geof:distance, geof:envelope, geof:geometryType, geof:getSRID, geof:intersection, geof:is3D, geof: isEmpty, geof:isMeasured, geof:isSimple, geof:spatialDimension, geof:symDifference, geof:transform und geof:union als SPARQL-Erweiterungsfunktionen, konsistent mit den Definitionen dieser Funktionen in Simple Features [ISO19125-1], für Nicht-DGGS-Geometrieliterale, unterstützen"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-non-sf rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementierungen müssen die Funktionen geof:area, geof:geometryN, geof:length, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY, geof:minZ, geof:numGeometries und geof:projectTo als SPARQL-Erweiterungsfunktionen, die in diesem Standard definiert sind, für Nicht-DGGS-Geometrieliterale unterstützen"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementierungen müssen die Funktionen der Anforderung 40 für DGGS-Geometrieliterale als SPARQL-Erweiterungsfunktionen in Übereinstimmung mit den Definitionen dieser Funktionen in Simple Features <<ISO19125-1>> für Nicht-DGGS-Geometrieliterale unterstützen"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .    

reqs11gx:query-functions-non-sf-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementierungen müssen die Funktionen der Anforderung 41 für DGGS-Geometrieliterale als SPARQL-Erweiterungsfunktionen für Nicht-DGGS-Geometrieliterale unterstützen"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .                                                                       

reqs11gx:sa-functions rdf:type 
                               spec:Requirement ;
                      skos:definition "Implementierungen müssen geof:concaveHull, geof:boundingCircle, geof:union2, geof:concatLines, geof:centroid, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY und geof:minZ als SPARQL-Erweiterungsfunktionen unterstützen."@de ;
                      skos:prefLabel "GeoSPARQL 1.1 Anforderung: Spatial Aggregate Funktionen"@de .
