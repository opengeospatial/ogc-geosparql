@prefix : <http://www.opengis.net/ont/sf3d#> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdo: <https://schema.org/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix app: <http://www.opengis.net/ont/app> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.opengis.net/ont/app> .

###  http://www.opengis.net/ont/sf3d#ambientIntensity
app:ambientIntensity rdf:type owl:DatatypeProperty ;
                      rdfs:domain app:Material ;
                      rdfs:range xsd:float ;
                      rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                      skos:definition "Reflection value of ambient light between 0 and 1"@en ;
                      skos:prefLabel "ambient Intensity"@en .


###  http://www.opengis.net/ont/sf3d#diffuseColor
app:diffuseColor rdf:type owl:DatatypeProperty ;
                  rdfs:domain app:Material ;
                  rdfs:range app:rgbaLiteral ;
                  rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                  skos:definition "Reflective color value"@en ;
                  skos:prefLabel "diffuse color"@en .


###  http://www.opengis.net/ont/sf3d#emissiveColor
app:emissiveColor rdf:type owl:DatatypeProperty ;
                   rdfs:domain app:Material ;
                   rdfs:range app:rgbaLiteral ;
                   rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                   skos:definition "Color value for glowing objects"@en ;
                   skos:prefLabel "emissive color"@en .

###  http://www.opengis.net/ont/sf3d#shininess
app:shininess rdf:type owl:DatatypeProperty ;
               rdfs:domain app:Material ;
               rdfs:range xsd:double ;
               rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
               skos:definition "Shininess value between 0 and 1 indicating the highlight intensity"@en ;
               skos:prefLabel "shininess"@en .


###  http://www.opengis.net/ont/sf3d#specularColor
app:specularColor rdf:type owl:DatatypeProperty ;
                   rdfs:domain app:Material ;
                   rdfs:range app:rgbaLiteral ;
                   rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                   skos:definition "Color value for shiny spots"@en ;
                   skos:prefLabel "specular color"@en .


###  http://www.opengis.net/ont/sf3d#transparency
app:transparency rdf:type owl:DatatypeProperty ;
                  rdfs:domain app:Material ;
                  rdfs:range xsd:double ;
                  rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                  skos:definition "Indicates the opacity of an object with 1.0 being completely transparent and 0.0 being completely opaque"@en ;
                  skos:prefLabel "transparency"@en .

###  http://www.opengis.net/ont/sf3d#Material
app:Material rdf:type owl:Class ;
              rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
              skos:definition "A coloring and  of a surface"@en ;
              skos:prefLabel "Material"@en .

###  http://www.opengis.net/ont/sf3d#Texture
app:Texture rdf:type owl:Class ;
             rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
             skos:definition "an image positioned on a defined position on the surface of a 3D solid"@en ;
             skos:prefLabel "Texture"@en .

###  http://www.opengis.net/ont/sf3d#Appearance
app:Appearance rdf:type owl:Class ;
                rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                skos:definition "The appearance of a surface"@en ;
                skos:prefLabel "Appearance"@en .

###  http://www.opengis.net/ont/sf3d#rgbaLiteral
app:rgbaLiteral rdf:type rdfs:Datatype ;
                 rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                 skos:definition "a RGBA color value represented as four double values"@en ;
                 skos:prefLabel "RGBA literal"@en .