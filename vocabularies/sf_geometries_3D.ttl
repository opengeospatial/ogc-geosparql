#################################################################
#    Object Properties
#################################################################

###  http://www.opengis.net/ont/sf3d#baseGeometry
sf3d:baseGeometry rdf:type owl:ObjectProperty ;
                  rdfs:domain sf3d:SolidGeometry ;
                  rdfs:range sf3d:Face ;
                  rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                  skos:definition "Relates a 3d object to its base surface geometry"@en ;
                  skos:prefLabel "base Geometry"@en .


###  http://www.opengis.net/ont/sf3d#faces
sf3d:faces rdf:type owl:ObjectProperty ;
           rdfs:domain sf3d:SolidGeometry ;
           rdfs:range sf3d:Face ;
           rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
           skos:definition "Relates a 3d solid to a face"@en ;
           skos:prefLabel "faces"@en .


###  http://www.opengis.net/ont/sf3d#hasAppearance
sf3d:hasAppearance rdf:type owl:ObjectProperty ;
                   rdfs:domain sf3d:SolidGeometry ;
                   rdfs:range sf3d:Material ;
                   rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                   skos:definition "Relates a 3D object to an instance of material or texture"@en ;
                   skos:prefLabel "base Geometry"@en .


###  http://www.opengis.net/ont/sf3d#metricXLength
sf3d:metricXLength rdf:type owl:ObjectProperty ;
                   rdfs:domain geo:Geometry .


###  http://www.opengis.net/ont/sf3d#metricYLength
sf3d:metricYLength rdf:type owl:ObjectProperty ;
                   rdfs:domain geo:Geometry .


###  http://www.opengis.net/ont/sf3d#metricZLength
sf3d:metricZLength rdf:type owl:ObjectProperty ;
                   rdfs:domain geo:Geometry .


###  http://www.opengis.net/ont/sf3d#orientation
sf3d:orientation rdf:type owl:ObjectProperty ;
                 rdfs:domain sf3d:SolidGeometry .


###  http://www.opengis.net/ont/sf3d#position
sf3d:position rdf:type owl:ObjectProperty ;
              rdfs:domain sf3d:SolidGeometry .


###  http://www.opengis.net/ont/sf3d#rotation
sf3d:rotation rdf:type owl:ObjectProperty ;
              rdfs:domain sf3d:SolidGeometry ;
              rdfs:range sf3d:matrixLiteral ;
              rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
              skos:definition "Rotation vector to orient a 3D model"@en ;
              skos:prefLabel "rotation"@en .


###  http://www.opengis.net/ont/sf3d#scale
sf3d:scale rdf:type owl:ObjectProperty ;
           rdfs:domain sf3d:SolidGeometry .


###  http://www.opengis.net/ont/sf3d#textures
sf3d:textures rdf:type owl:ObjectProperty ;
              rdfs:domain sf3d:SolidGeometry ;
              rdfs:range sf3d:Texture ;
              rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
              skos:definition "Relates a 3d solid to a texture"@en ;
              skos:prefLabel "textures"@en .


###  http://www.opengis.net/ont/sf3d#translation
sf3d:translation rdf:type owl:ObjectProperty ;
                 rdfs:domain sf3d:SolidGeometry ;
                 rdfs:range sf3d:matrixLiteral ;
                 rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
                 skos:definition "Translation vector to orient a 3D model"@en ;
                 skos:prefLabel "translation"@en .


###  http://www.opengis.net/ont/sf3d#vertices
sf3d:vertices rdf:type owl:ObjectProperty ;
