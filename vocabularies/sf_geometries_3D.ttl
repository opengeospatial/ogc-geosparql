sf3d:plyLiteral
    a
       rdfs:Datatype ;
    rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
    skos:definition """A literal which includes a 3D object in the PLY ASCII format"""@en ;
    skos:prefLabel "PLY literal"@en ;
.

sf3d:matrixLiteral
    a
       rdfs:Datatype ;
    rdfs:isDefinedBy <http://www.opengis.net/ont/sf3d> ;
