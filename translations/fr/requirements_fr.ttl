reqs11core:spatial-object-collection-class rdf:type 
                                                    spec:Requirement ;
                                           skos:definition "Les implémentations doivent autoriser la classe RDFS geo:SpatialObjectCollection a être utilisée dans les modèles de graphe SPARQL."@fr ;
                                           skos:prefLabel "Conditions GeoSPARQL 1.1: Classe d'objects géospatiales"@fr .

reqs11core:feature-collection-class rdf:type 
                                             spec:Requirement ;
                                    skos:definition "Les implémentations doivent autoriser la classe RDFS geo:FeatureCollection a être utilisée dans les modèles de graphe SPARQL."@fr ;
                                    skos:prefLabel "Conditions GeoSPARQL 1.1: Classe d'objects caractéristiques"@fr .

reqs11core:geometry-collection-class rdf:type 
                                              spec:Requirement ;
                                     skos:definition "Les implémentations doivent autoriser la classe RDFS geo:GeometryCollection a être utilisée dans les modèles de graphe SPARQL."@fr ;
                                     skos:prefLabel "Conditions GeoSPARQL 1.1: Classe collections géométriques"@fr .

reqs11core:sparql-protocol rdf:type 
                                    spec:Requirement ;
                           skos:definition "Les implémentations doivent prendre en charge la langue de requête SPARQL pour RDF [SPARQL], le protocole SPARQL [SPARQLPROT] et les Résultats de requête SPARQL en formats XML [SPARQLRESX] et JSON [SPARQLRESJ].."@fr ;
                           skos:prefLabel "Conditions GeoSPARQL 1.1: Protocole SPARQL"@fr ;
                           prov:wasDerivedFrom reqs10core:sparql-protocol .

reqs11gx:dggs-literal rdf:type 
                               spec:Requirement ;
                      skos:definition "Tous les littéraux RDFS de type geo:dggsLiteral doivent être constitués d'une série de géométrie DGGS formulée selon un type littéral DGGS spécifique identifié par un type de données spécialisant le type de données."@fr ;
                      skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral DGGS"@fr .

reqs11gx:dggs-literal-empty rdf:type 
                                     spec:Requirement ;
                            skos:definition "Un geo:dggsLiteral vide doit être interprété comme une géométrie vide."@fr ;
                            skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral DGGS Vide"@fr .

reqs11gx:feature-properties rdf:type 
                                     spec:Requirement ;
                            skos:definition "Les implémentations doivent permettre aux propriétés geo:hasGeometry, geo:hasDefaultGeometry, geo:hasLength, geo:hasArea, geo:hasVolume geo:hasCentroid, geo:hasBoundingBox et geo:hasSpatialResolution d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                            skos:prefLabel "Conditions GeoSPARQL 1.1: Propriétés caractéristiques"@fr ;
                            prov:wasDerivedFrom reqs10gx:feature-properties .

reqs11gx:geojson-literal rdf:type 
                                  spec:Requirement ;
                         skos:definition "Les instances geo:geoJSONLiteral sont des objets géométrie tels que définis dans la spécification Geojson [Geojson]."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral GeoJSON"@fr .

reqs11gx:geojson-literal-empty rdf:type 
                                        spec:Requirement ;
                               skos:definition "Un geo:kmlLiteral vide doit être interprété comme une géométrie vide."@fr ;
                               skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral KML Vide"@fr .

reqs11gx:geojson-literal-srs rdf:type 
                                      spec:Requirement ;
                             skos:definition "Les littéraux de type geo:geoJSONLiteral ne contiennent pas de définition SRS. Tous les littéraux de ce type doivent, selon la spécification Geojson, codés et utilisés, le système de référence spatial de la longitude géodésique WGS84 (http://www.opengis.net/def/CRS/OGC/1.3/CRS84)."@fr ;
                             skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral GeoJSON SRS par défault"@fr .

reqs11gx:geometry-as-dggs-function rdf:type 
                                            spec:Requirement ;
                                   skos:definition "Les implémentations doivent prendre en charge geof:asDGGS, en tant que fonction d'extension SPARQL."@fr ;
                                   skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction asDGGS"@fr .

reqs11gx:geometry-as-dggs-literal rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Les implémentations doivent permettre aux littéraux geo:asDGGS d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                  skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral géométrique asDGGS"@fr .

reqs11gx:geometry-as-geojson-function rdf:type 
                                               spec:Requirement ;
                                      skos:definition "Les implémentations doivent permettre aux fonctions geof:asGeoJSON d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                      skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction asGeoJSON"@fr .

reqs11gx:geometry-as-geojson-literal rdf:type 
                                              spec:Requirement ;
                                     skos:definition "Les implémentations doivent permettre aux littéraux geo:asGeoJSON d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                     skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral asGeoJSON"@fr .

reqs11gx:geometry-as-gml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition  "Les implémentations doivent permettre aux fonctions geof:asGML d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                  skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction asGML"@fr .

reqs11gx:geometry-as-kml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Les implémentations doivent permettre aux fonctions geof:asKML d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                  skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction asKML"@fr .

reqs11gx:geometry-as-kml-literal rdf:type 
                                          spec:Requirement ;
                                 skos:definition "Les implémentations doivent permettre aux littéraux geo:asKML d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                 skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral géométrique asKML"@fr .

reqs11gx:geometry-as-wkt-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Les implémentations doivent permettre aux fonctions geof:asWKT d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                                  skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction asWKT"@fr .

reqs11gx:geometry-properties rdf:type 
                                      spec:Requirement ;
                             skos:definition "Les implémentations doivent permettre aux propriétés geo:dimension, geo:coordinateDimension, geo:spatialDimension, geo:isEmpty, geo:isSimple et geo:hasSerialization d'être utilisées dans les modèles de graphe SPARQL."@fr ;
                             skos:prefLabel "Conditions GeoSPARQL 1.1: Propriétés géométriques"@fr ;
                             prov:wasDerivedFrom reqs10gx:geometry-properties .

reqs11gx:kml-literal rdf:type 
                              spec:Requirement ;
                     skos:definition "Toutes les instances geo:kmlLiteral doivent être composées des objets de géométrie définis dans la spécification KML [OGCKML]."@fr ;
                     skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral KML"@fr .

reqs11gx:kml-literal-empty rdf:type 
                                    spec:Requirement ;
                           skos:definition "Un geo:kmlLiteral vide doit être interprété comme une géométrie vide."@fr ;
                           skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral GeoJSON Vide"@fr .

reqs11gx:kml-literal-srs rdf:type 
                                  spec:Requirement ;
                         skos:definition "L'IRI http://www.opengis.net/def/crs/ogc/1.3/crrs84 doit être supposé pour change geo:kmlLiteral qui n'en spécifient pas."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Littéral KML SRS par défault"@fr .

reqs11gx:query-functions rdf:type 
                                  spec:Requirement ;
                         skos:definition "Les implémentations doivent permettre aux fonctions geof:boundary, geof:buffer, geof:convexHull, geof:coordinateDimension, geof:difference, geof:dimension, geof:distance, geof:envelope, geof:geometryType, geof:getSRID, geof:intersection, geof:is3D, geof:isEmpty, geof:isMeasured, geof:isSimple, geof:spatialDimension, geof:symDifference, geof:transform et geof:union d'être utilisées dans les modèles de graphe SPARQL selon les definitions Simple Features [ISO19125-1] pour les littéraux non-DGGS."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Requêtes fonctionnelles"@fr ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-non-sf rdf:type 
                                  spec:Requirement ;
                         skos:definition "Les implémentations doivent permettre aux fonctions geof:area, geof:geometryN, geof:length, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY, geof:minZ, geof:numGeometries et geof:projectTo d'être utilisées dans les modèles de graphe SPARQL selon les definitions de ce standard pour les littéraux non-DGGS."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Requêtes fonctionnelles"@fr ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Les implémentations doivent prendre en charge les fonctions d'exigence 40 pour les littéraux de la géométrie DGGS en tant que fonctions d'extension SPARQL, conformément aux définitions Simple Features [ISO19125-1] pour les littéraux de géométrie non-DGGS."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Requêtes fonctionnelles"@fr ;
                         prov:wasDerivedFrom reqs10gx:query-functions .    

reqs11gx:query-functions-non-sf-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Les implémentations doivent prendre en charge les fonctions d'exigence 41 pour les littéraux de la géométrie DGGS en tant que fonctions d'extension SPARQL, conformément aux définitions de ce standard pour les littéraux de géométrie non-DGGS."@fr ;
                         skos:prefLabel "Conditions GeoSPARQL 1.1: Requêtes fonctionnelles"@fr ;
                         prov:wasDerivedFrom reqs10gx:query-functions .                                                                       

reqs11gx:sa-functions rdf:type 
                               spec:Requirement ;
                      skos:definition "Les implémentations doivent permettre aux fonctions geof:concaveHull, geof:boundingCircle, geof:union2, geof:concatLines, geof:centroid, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY et geof:minZ en tant que fonctions d'extension SPARQL."@fr ;
                      skos:prefLabel "Conditions GeoSPARQL 1.1: Fonction Agrégat spatiales"@fr .
