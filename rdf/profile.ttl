PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX role: <http://www.w3.org/ns/dx/prof/role/>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<http://www.opengis.net/def/geosparql>
    a prof:Profile ;
    owl:versionIRI <http://www.opengis.net/def/geosparql/1.3> ;
    owl:versionInfo "OGC GeosPARQL 1.3" ;
    skos:historyNote "2026-01: NJC- Modified to link directly to resources in the OGC Rainbow and remove resources never implemented" ;
    skos:scopeNote "Profile declarations are used to allow data to identify specifications in a way that allows data to make conformance claims to them - a broader conformance claim than that made to individual 'conformance classes' that are commonly found in recent OGC specifications. Profile declarations are also used to describe and relate the often multiple parts of specifications: ontologies, specification 'documents', vocabularies, validators and so on. This declaration describes where those parts are, what form (format) they take, what information models they implement and what roles they play."@en ;
    prof:hasResource 
        [
            a prof:ResourceDescriptor ;
            dcterms:format "text/json" ;
            prof:hasArtifact "https://github.com/opengeospatial/ogc-geosparql/blob/geosparql-1.3/contexts/sf-context.json"^^xsd:anyURI ;
            prof:hasRole role:specification ;
            schema:description "This is a JSON-LD context which can be used to incorporate the Simple Features vocabulary in a JSON-LD file"@en ;
            schema:name "JSON-LD context for the Simple Features vocabulary"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://linked.data.gov.au/def/vocpub> ;
            prof:hasArtifact <http://www.opengis.net/def/rule/geosparql> ;
            prof:hasRole role:vocabulary ;
            schema:description "All GeoSPARQL rules  presented as a SKOS vocabulary"@en ;
            schema:name "GeoSPARQL Rules vocabulary"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:format "text/html" ;
            prof:hasArtifact "https://docs.ogc.org/is/22-047r1/22-047r1.html"^^xsd:anyURI ;
            prof:hasRole role:specification ;
            schema:name "GeoSPARQL Specification"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://linked.data.gov.au/def/ontpub> ;
            prof:hasArtifact <http://www.opengis.net/ont/geosparql> ;
            prof:hasRole role:schema ;
            schema:description "An OWL ontology of the GeoSPARQL model"@en ;
            schema:name "GeoSPARQL Ontology"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://linked.data.gov.au/def/vocpub> ;
            prof:hasArtifact <http://www.opengis.net/def/function/geosparql> ;
            prof:hasRole role:vocabulary ;
            schema:description "All GeoSPARQL functions presented as a SKOS vocabulary"@en ;
            schema:name "GeoSPARQL Functions vocabulary"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            prof:hasArtifact "https://github.com/opengeospatial/ogc-geosparql/tree/geosparql-1.3/examples"^^xsd:anyURI ;
            prof:hasRole role:example ;
            schema:description "Examples that are too long to sensibly fit within the Specification's Annex B. They are presented as individual files within a version control repository directory."@en ;
            schema:name "Extended Examples"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://www.w3.org/TR/shacl/> ;
            prof:hasArtifact <http://www.opengis.net/def/geosparql/validator> ;
            prof:hasRole role:validation ;
            schema:description "A [Shapes Constraint Language (SHACL)](https://www.w3.org/TR/shacl/) file that specifies rules for RDF data that can be used to test the conformance of data to the GeoSPARQL standard."@en ;
            schema:name "GeoSPARQL RDF Shapes Validator"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            prof:hasArtifact "https://github.com/opengeospatial/ogc-geosparql"^^xsd:anyURI ;
            prof:hasRole role:repository ;
            schema:description "Code repository storing all Profile artifacts"@en ;
            schema:name "Profile code repository"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:format "text/json" ;
            prof:hasArtifact "https://github.com/opengeospatial/ogc-geosparql/blob/geosparql-1.3/contexts/geo-context.json"^^xsd:anyURI ;
            prof:hasRole role:specification ;
            schema:description "This is a JSON-LD context which can be used to incorporate the GeoSPARQL vocabulary in a JSON-LD file"@en ;
            schema:name "JSON-LD context for the GeoSPARQL ontology"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://linked.data.gov.au/def/ontpub> ;
            prof:hasArtifact <http://www.opengis.net/ont/sf> ;
            prof:hasRole role:schema ;
            schema:description "An OWL ontology of the Simple Features geometry types"@en ;
            schema:name "Simple Features Ontology"@en ;
        ] ,
        [
            a prof:ResourceDescriptor ;
            dcterms:conformsTo <https://linked.data.gov.au/def/vocpub> ;
            prof:hasArtifact <http://www.opengis.net/def/geosparql/reqs> ;
            prof:hasRole role:vocabulary ;
            schema:description "A vocabulary of the requirements and conformance classes in the GeoSPARQL 1.3 specification"@en ;
            schema:name "GeoSPARQL 1.3 Requirement and Conformance Class Vocabulary"@en ;
        ] ;
    schema:copyrightNotice "(c) 2021 Open Geospatial Consortium" ;
    schema:creator <https://defs.opengis.net/org/geosparql-swg> ;
    schema:dateCreated "2020-10-11"^^xsd:date ;
    schema:dateModified "2026-02-04"^^xsd:date ;
    schema:description "This is a 'profile declaration' for the GeoSPARQL 1.3 specification (standard). It describes the multiple parts of the specification and how the standard relates to other standards. It is formulated according to the Profiles Vocabulary."@en ;
    schema:license "https://www.ogc.org/license"^^xsd:anyURI ;
    schema:name "GeoSPARQL 1.3 Profile" ;
    schema:publisher <https://defs.opengis.net/org/ogc> ;
.

