ex:PlaceOfInterest a rdfs:Class ;
    rdfs:subClassOf geo:Feature .

<http://www.opengis.net/ont/gml#Polygon> a rdfs:Class ;
    rdfs:subClassOf <http://www.opengis.net/ont/gml#Surface> .

<http://www.opengis.net/ont/sf#LineString> a rdfs:Class ;
    rdfs:subClassOf <http://www.opengis.net/ont/sf#Curve> .

<http://www.opengis.net/ont/sf#Polygon> a rdfs:Class ;
    rdfs:subClassOf <http://www.opengis.net/ont/sf#Surface> .

ex:hasExactGeometry a rdf:Property ;
    rdfs:subPropertyOf geo:hasDefaultGeometry,
        geo:hasGeometry .

ex:hasPointGeometry a rdf:Property ;
    rdfs:subPropertyOf geo:hasGeometry .

