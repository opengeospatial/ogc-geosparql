@prefix safuncs: <http://www.opengis.net/def/function/aggregate/geosparql/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

safuncs: a skos:Collection ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:member
        safuncs:bbox,
        safuncs:boundingcircle,
        safuncs:centroid,
        safuncs:concatlines,
        safuncs:concavehull,
        safuncs:convexhull,
        safuncs:union;
    skos:prefLabel "GeoSPARQL Spatial Aggregate Functions" .

safuncs:bbox a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that aggregates geometries within a specified bounding box."@en ;
    skos:prefLabel "bbox"@en .

safuncs:boundingcircle a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that aggregates geometries within a specified bounding circle."@en ;
    skos:prefLabel "bounding circle"@en .

safuncs:centroid a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that calculates a centroid out of a list of vector geometries."@en ;
    skos:prefLabel "centroid"@en .

safuncs:concatlines a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that calculates a concatenated line out of a list of lines."@en ;
    skos:prefLabel "concat lines"@en .

safuncs:concavehull a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that calculates the concave hull of a list of vector geometries."@en ;
    skos:prefLabel "concave hull"@en .

safuncs:convexhull a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that calculates the convex hull out of a list of vector geometries."@en ;
    skos:prefLabel "convex hull"@en .

safuncs:union a skos:Concept ;
    dcterms:contributor "Timo Homburg" ;
    dcterms:creator "OGC GeoSPARQL 2.0 Standard Working Group" ;
    dcterms:date "2021-02-25"^^xsd:date ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    policy:status status:valid ;
    rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:definition "A spatial aggregate function that calculates the union out of a list of vector geometries."@en ;
    skos:prefLabel "union"@en .
